{
  "version": "1.0",
  "name": "asean picker",
  "template": "{\"type\":\"Card\",\"size\":\"md\",\"children\":[{\"type\":\"Col\",\"gap\":2,\"children\":[{\"type\":\"Title\",\"value\":{{ (question) | tojson }},\"size\":\"sm\"},{\"type\":\"Text\",\"value\":\"Tap an option to pick, or write your own.\",\"size\":\"sm\",\"color\":\"secondary\"}]},{\"type\":\"Divider\",\"flush\":true},{\"type\":\"ListView\",\"children\":[{%- set _c -%}{%-for item in options -%},{\"type\":\"ListViewItem\",\"key\":{{ (item.id) | tojson }},\"gap\":3,\"onClickAction\":{\"type\":\"asean.select\",\"payload\":{\"id\":{{ (item.id) | tojson }}},\"handler\":\"client\"},\"children\":[{\"type\":\"Col\",\"gap\":1,\"children\":[{\"type\":\"Row\",\"gap\":2,\"children\":[{\"type\":\"Badge\",\"label\":{{ (item.tag) | tojson }},\"color\":\"discovery\"},{\"type\":\"Text\",\"value\":{{ (item.title) | tojson }},\"size\":\"sm\",\"weight\":\"semibold\"}]},{\"type\":\"Text\",\"value\":{{ (item.description) | tojson }},\"size\":\"sm\",\"color\":\"secondary\"}]},{\"type\":\"Spacer\"},{\"type\":\"Icon\",\"name\":\"chevron-right\",\"color\":\"secondary\"}]}{%-endfor-%}{%- endset -%}{{- (_c[1:] if _c and _c[0] == ',' else _c) -}}]},{\"type\":\"Divider\",\"flush\":true},{\"type\":\"Form\",\"onSubmitAction\":{\"type\":\"asean.custom_submit\",\"handler\":\"client\"},\"children\":[{\"type\":\"Col\",\"gap\":2,\"children\":[{\"type\":\"Label\",\"value\":\"Or propose your own\",\"fieldName\":\"custom\"},{\"type\":\"Textarea\",\"name\":\"custom\",\"placeholder\":{{ (customPlaceholder) | tojson }},\"rows\":4,\"autoResize\":true},{\"type\":\"Row\",\"children\":[{\"type\":\"Spacer\"},{\"type\":\"Button\",\"submit\":true,\"style\":\"primary\",\"label\":\"Submit\"}]}]}]}]}",
  "jsonSchema": {
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
      "question": {
        "type": "string"
      },
      "options": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string"
            },
            "tag": {
              "type": "string"
            },
            "title": {
              "type": "string"
            },
            "description": {
              "type": "string"
            }
          },
          "required": [
            "id",
            "tag",
            "title",
            "description"
          ],
          "additionalProperties": false
        }
      },
      "customPlaceholder": {
        "type": "string"
      }
    },
    "required": [
      "question",
      "options",
      "customPlaceholder"
    ],
    "additionalProperties": false
  },
  "outputJsonPreview": {
    "type": "Card",
    "size": "md",
    "children": [
      {
        "type": "Col",
        "gap": 2,
        "children": [
          {
            "type": "Title",
            "value": "What if I need to expand GEM to ASEAN countries?",
            "size": "sm"
          },
          {
            "type": "Text",
            "value": "Tap an option to pick, or write your own.",
            "size": "sm",
            "color": "secondary"
          }
        ]
      },
      {
        "type": "Divider",
        "flush": true
      },
      {
        "type": "ListView",
        "children": [
          {
            "type": "ListViewItem",
            "key": "A",
            "gap": 3,
            "onClickAction": {
              "type": "asean.select",
              "payload": {
                "id": "A"
              },
              "handler": "client"
            },
            "children": [
              {
                "type": "Col",
                "gap": 1,
                "children": [
                  {
                    "type": "Row",
                    "gap": 2,
                    "children": [
                      {
                        "type": "Badge",
                        "label": "Option A",
                        "color": "discovery"
                      },
                      {
                        "type": "Text",
                        "value": "Thailand hub + country sales (lightweight)",
                        "size": "sm",
                        "weight": "semibold"
                      }
                    ]
                  },
                  {
                    "type": "Text",
                    "value": "Keep solutioning, partner-China mgmt, standards, commissioning SWAT, and delivery governance in Thailand. Start with 1 BD/KAM per target country; build local delivery later.",
                    "size": "sm",
                    "color": "secondary"
                  }
                ]
              },
              {
                "type": "Spacer"
              },
              {
                "type": "Icon",
                "name": "chevron-right",
                "color": "secondary"
              }
            ]
          },
          {
            "type": "ListViewItem",
            "key": "B",
            "gap": 3,
            "onClickAction": {
              "type": "asean.select",
              "payload": {
                "id": "B"
              },
              "handler": "client"
            },
            "children": [
              {
                "type": "Col",
                "gap": 1,
                "children": [
                  {
                    "type": "Row",
                    "gap": 2,
                    "children": [
                      {
                        "type": "Badge",
                        "label": "Option B",
                        "color": "discovery"
                      },
                      {
                        "type": "Text",
                        "value": "Regional hub + country business units (scalable)",
                        "size": "sm",
                        "weight": "semibold"
                      }
                    ]
                  },
                  {
                    "type": "Text",
                    "value": "Add an ASEAN Regional Director; create small ‘mini‑GEM’ teams per country (Commercial + Delivery/O&M); run a regional CoE for standards, pre‑sales, commissioning, and QHSE.",
                    "size": "sm",
                    "color": "secondary"
                  }
                ]
              },
              {
                "type": "Spacer"
              },
              {
                "type": "Icon",
                "name": "chevron-right",
                "color": "secondary"
              }
            ]
          },
          {
            "type": "ListViewItem",
            "key": "C",
            "gap": 3,
            "onClickAction": {
              "type": "asean.select",
              "payload": {
                "id": "C"
              },
              "handler": "client"
            },
            "children": [
              {
                "type": "Col",
                "gap": 1,
                "children": [
                  {
                    "type": "Row",
                    "gap": 2,
                    "children": [
                      {
                        "type": "Badge",
                        "label": "Option C",
                        "color": "discovery"
                      },
                      {
                        "type": "Text",
                        "value": "Partner‑led delivery (asset‑light)",
                        "size": "sm",
                        "weight": "semibold"
                      }
                    ]
                  },
                  {
                    "type": "Text",
                    "value": "GEM owns sales, solution design, and performance governance; local EPC/O&M partners deliver under GEM standards.",
                    "size": "sm",
                    "color": "secondary"
                  }
                ]
              },
              {
                "type": "Spacer"
              },
              {
                "type": "Icon",
                "name": "chevron-right",
                "color": "secondary"
              }
            ]
          }
        ]
      },
      {
        "type": "Divider",
        "flush": true
      },
      {
        "type": "Form",
        "onSubmitAction": {
          "type": "asean.custom_submit",
          "handler": "client"
        },
        "children": [
          {
            "type": "Col",
            "gap": 2,
            "children": [
              {
                "type": "Label",
                "value": "Or propose your own",
                "fieldName": "custom"
              },
              {
                "type": "Textarea",
                "name": "custom",
                "placeholder": "Write your own approach (100–200 words)…",
                "rows": 4,
                "autoResize": true
              },
              {
                "type": "Row",
                "children": [
                  {
                    "type": "Spacer"
                  },
                  {
                    "type": "Button",
                    "submit": true,
                    "style": "primary",
                    "label": "Submit"
                  }
                ]
              }
            ]
          }
        ]
      }
    ]
  },
